{
    "name": "react-image-slider",
    "version": "0.0.1",
    "description": "A Dynamic Image Slider created with React",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "clean": "rimraf dist",
        "cleanSrc": "rimraf dist/src",
        "serve": "webpack-dev-server --config webpack-dev.config.js && TARGET_ENV=development",
        "build": "npm run clean && webpack --config webpack-prod.config.js",
        "deploy": "git subtree push --prefix dist origin gh-pages"
    },
    "jest": {
        "setupFiles": [
            "raf/polyfill"
        ],
        "moduleNameMapper": {
            "\\.(pdf|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "identity-obj-proxy"
        }
    },
    "author": "Maria D. Campbell",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^7.1.6",
        "babel-core": "^6.26.0",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-react-jsx-img-import": "^0.1.4",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "css-hot-loader": "^1.3.3",
        "css-loader": "^0.28.7",
        "enzyme": "^3.1.1",
        "eslint": "^4.11.0",
        "eslint-plugin-react": "^7.4.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "html-webpack-plugin": "^2.30.1",
        "identity-obj-proxy": "^3.0.0",
        "image-webpack-loader": "^3.4.2",
        "imagemin-webpack-plugin": "^1.5.2",
        "jest": "^21.2.1",
        "jest-enzyme": "^4.0.1",
        "json-loader": "^0.5.7",
        "node-sass": "^4.6.0",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "postcss": "^6.0.14",
        "postcss-loader": "^2.0.8",
        "prop-types": "^15.6.0",
        "raf": "^3.4.0",
        "react-test-renderer": "^16.1.0",
        "regenerator-runtime": "^0.11.0",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.0",
        "uglifyjs-webpack-plugin": "^1.0.1",
        "url-loader": "^0.6.2",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.4"
    },
    "dependencies": {
        "core-js": "^2.5.1",
        "radium": "^0.19.6",
        "react": "^16.1.1",
        "react-dom": "^16.1.1"
    }
}
